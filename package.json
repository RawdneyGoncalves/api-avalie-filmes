{
  "name": "api-avalie-filmes",
  "version": "1.0.0",
  "description": "API de Avaliação de Filmes usando Fastify e TypeORM",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc-watch --onSuccess \"node dist/app.js\"",
    "start": "node dist/app.js",
    "test": "jest --config tsconfig.test.json",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d ./src/config/ormconfig.ts -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "fastify-oas": "^3.0.8",
    "inversify": "^6.0.2",
    "jest": "^29.7.0",
    "mysql2": "^3.11.3",
    "openapi-types": "^12.1.3",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.0.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
